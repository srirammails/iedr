<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ieapi xmlns="http://www.domainregistry.ie/ieapi-1.3"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.domainregistry.ie/ieapi-1.3
     ieapi-1.3.xsd">
    <command>
        <modify>
            <domain:modify
                    xmlns:domain="http://www.domainregistry.ie/ieapi-domain-1.3"
                    xsi:schemaLocation="http://www.domainregistry.ie/ieapi-domain-1.3
       			ieapi-domain-1.3.xsd">
                <domain:name>thedomena.tomodify3.ie</domain:name>
                <domain:add>
                    <domain:ns>
                        <domain:nsName>ns1.TheDomena.ToModify3.ie</domain:nsName>
                        <domain:nsAddr>1.2.3.4</domain:nsAddr>
                    </domain:ns>
                    <domain:ns>
                        <domain:nsName>ns2.TheDomena.ToModify3.ie</domain:nsName>
                        <domain:nsAddr>1.2.3.4</domain:nsAddr>
                    </domain:ns>
                    <domain:contact type="admin">APIT2-IEDR</domain:contact>
                    <domain:contact type="tech">APIT1-IEDR</domain:contact>
                </domain:add>
                <domain:rem>
                    <domain:ns>
                        <domain:nsName>dns1.myhostns.com</domain:nsName>
                    </domain:ns>
                    <domain:contact type="admin">APIT1-IEDR</domain:contact>
                    <domain:contact type="tech">APIT2-IEDR</domain:contact>
                </domain:rem>
                <domain:chg>
                    <domain:holder class="natural person">
                        <domain:holderName category="personal name">
                            John Doe
                        </domain:holderName>
                        <domain:holderRemarks/>
                    </domain:holder>
                    <!--<domain:renewalMode>NoAutorenew</domain:renewalMode>-->
                </domain:chg>
            </domain:modify>
        </modify>
    </command>
</ieapi>
