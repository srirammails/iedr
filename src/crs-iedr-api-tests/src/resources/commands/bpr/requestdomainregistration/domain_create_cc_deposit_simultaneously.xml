<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ieapi xmlns="http://www.domainregistry.ie/ieapi-1.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.domainregistry.ie/ieapi-1.3
	ieapi-1.3.xsd">
	<command>
		<create>
			<domain:create
				xmlns:domain="http://www.domainregistry.ie/ieapi-domain-1.3"
				xsi:schemaLocation="http://www.domainregistry.ie/ieapi-domain-1.3
				ieapi-domain-1.3.xsd">
				<domain:name>createDomainRegistrarBasic1.ie</domain:name>
				<domain:period unit="y">1</domain:period>
				<domain:ns>
					<domain:nsName>ns1.createDomainRegistrarBasic1.ie</domain:nsName>
					<domain:nsAddr>192.1.2.3</domain:nsAddr>
				</domain:ns>
				<domain:ns>
					<domain:nsName>ns1.example-nameserver.ie</domain:nsName>
				</domain:ns>
				<domain:holder class="Natural Person">
					<domain:holderName category="Personal Name">
						John Doe
					</domain:holderName>
					<domain:holderRemarks />
				</domain:holder>
				<domain:contact type="admin">APIT1-IEDR</domain:contact>
				<domain:contact type="tech">APIT2-IEDR</domain:contact>
                <domain:creditCard>
                        <domain:cardHolderName>John Doe</domain:cardHolderName>
                        <domain:cardNumber>4263971921001307</domain:cardNumber>
                        <domain:expiryDate>2015-01</domain:expiryDate>
                        <domain:cardType>VISA</domain:cardType>
                </domain:creditCard>
                <domain:payFromDeposit/>
			</domain:create>
		</create>
	</command>
</ieapi>