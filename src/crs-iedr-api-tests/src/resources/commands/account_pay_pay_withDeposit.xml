<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ieapi xmlns="http://www.domainregistry.ie/ieapi-1.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.domainregistry.ie/ieapi-1.3
	ieapi-1.3.xsd">
	<command>
		<pay>
			<account:pay
				xmlns:account="http://www.domainregistry.ie/ieapi-account-1.3"
				xsi:schemaLocation="http://www.domainregistry.ie/ieapi-account-1.3
				ieapi-account-1.3.xsd">
				
				<account:domain>domain1.ie</account:domain>
				<account:method>
                    <account:deposit/>
                </account:method>
				<account:period>1</account:period>
            </account:pay>
		</pay>
	</command>
</ieapi>
