	xsi:schemaLocation="http://www.domainregistry.ie/ieapi-1.3
     ieapi-1.3.xsd">
	<command>
		<modify>
			<domain:modify
				xmlns:domain="http://www.domainregistry.ie/ieapi-domain-1.3"
				xsi:schemaLocation="http://www.domainregistry.ie/ieapi-domain-1.3
       			ieapi-domain-1.3.xsd">
				<domain:name>thedomena.tomodify1.ie</domain:name>
				<domain:add>
					<domain:ns>
						<domain:nsName>ns1.example.ie</domain:nsName>
					</domain:ns>
				</domain:add>
				<domain:rem>
					<domain:ns>
						<domain:nsName>dns1.myhostns.com</domain:nsName>
					</domain:ns>
				</domain:rem>
			</domain:modify>
		</modify>
	</command>
</ieapi>

