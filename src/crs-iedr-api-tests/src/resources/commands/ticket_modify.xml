<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ieapi xmlns="http://www.domainregistry.ie/ieapi-1.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.domainregistry.ie/ieapi-1.3
     ieapi-1.3.xsd">
	<command>
		<modify>
			<ticket:modify
				xmlns:ticket="http://www.domainregistry.ie/ieapi-ticket-1.3"
				xsi:schemaLocation="http://www.domainregistry.ie/ieapi-ticket-1.3
       			ieapi-ticket-1.3.xsd">
				<ticket:name>tickettomodify.ie</ticket:name>
				<ticket:add>
					<ticket:ns>
						<ticket:nsName>ns11.tickettomodify.ie</ticket:nsName>
						<ticket:nsAddr>192.1.2.3</ticket:nsAddr>
					</ticket:ns>
                    <ticket:contact type="admin">APIT1-IEDR</ticket:contact>
                    <ticket:contact type="tech">APIT2-IEDR</ticket:contact>
				</ticket:add>
				<ticket:rem>
					<ticket:ns>
						<ticket:nsName>ns1.brianserver.com</ticket:nsName>
					</ticket:ns>
                    <ticket:contact type="admin">APIT2-IEDR</ticket:contact>
                    <ticket:contact type="tech">APIT1-IEDR</ticket:contact>
                </ticket:rem>
				<ticket:chg>
					<ticket:holder class="Natural Person">
						<ticket:holderName category="Personal Name">
							John Doe
						</ticket:holderName>
						<ticket:holderRemarks />
					</ticket:holder>
				</ticket:chg>
			</ticket:modify>
		</modify>
	</command>
</ieapi>
