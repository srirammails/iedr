<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ieapi xmlns="http://www.domainregistry.ie/ieapi-1.4"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.domainregistry.ie/ieapi-1.4 ieapi-1.4.xsd ">
	<command>
		<create>
			<domain:create
				xmlns:domain="http://www.domainregistry.ie/ieapi-domain-1.4"
				xsi:schemaLocation="http://www.domainregistry.ie/ieapi-domain-1.4
				ieapi-domain-1.4.xsd">
				<domain:name>exampleWWW.ie</domain:name>
				<domain:period unit="y">1</domain:period>
				<domain:ns>
					<domain:nsName>ns1.exampleWWW.ie</domain:nsName>
					<domain:nsAddr>192.1.2.3</domain:nsAddr>
				</domain:ns>
				<domain:ns>
					<domain:nsName>ns1.example2.ie</domain:nsName>
				</domain:ns>
				<domain:holder class="Natural Person">
					<domain:holderName category="Personal Name">
						John Doe
					</domain:holderName>
					<domain:holderRemarks />
				</domain:holder>
				<domain:contact type="admin">APITS2-IEDR</domain:contact>
				<domain:contact type="tech">APITS2-IEDR</domain:contact>
				<secDNS:create xmlns:secDNS="urn:ietf:params:xml:ns:secDNS-1.1">
					<secDNS:maxSigLife>10</secDNS:maxSigLife>
				</secDNS:create>
			</domain:create>
		</create>
	</command>
</ieapi>
